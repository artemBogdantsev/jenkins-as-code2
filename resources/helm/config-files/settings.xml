<settings>
    <interactiveMode>false</interactiveMode>
    <localRepository>/home/jenkins/.m2/repository</localRepository>
    <servers>
        <server>
            <id>norcom.internal.repo</id>
            <username>admin</username>
            <password>admin123</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
        </server>
        <server>
            <id>norcom.internal.snapshot.repo</id>
            <username>admin</username>
            <password>admin123</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
        </server>
    </servers>

    <profiles>
        <profile>
            <id>norcom</id>

            <properties>
                <altDeploymentRepository>norcom.internal.snapshot.repo::default::http://nexus-service:8081/repository/eagle-snapshots</altDeploymentRepository>
                <altReleaseDeploymentRepository>norcom.internal.repo::default::http://nexus-service:8081/repository/eagle-releases</altReleaseDeploymentRepository>
            </properties>

            <!-- Configuration of the Norcom repository used from within the company -->
            <repositories>
                <repository>
                    <id>norcom.internal.repo</id>
                    <name>NorCom Internal Repository</name>
                    <url>http://nexus-service:8081/repository/eagle-releases/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>norcom.internal.repo.old</id>
                    <name>Old NorCom Internal Repository</name>
                    <url>http://172.19.1.55:8081/repository/eagle-releases/</url>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>never</updatePolicy>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>never</updatePolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>norcom.internal.snapshot.repo</id>
                    <name>NorCom Internal Snapshot Repository</name>
                    <url>http://nexus-service:8081/repository/eagle-snapshots</url>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>norcom</activeProfile>
    </activeProfiles>

</settings>